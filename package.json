{
 "name": "yourapp",
 "version": "0.1.0",
 "private": true,
 "scripts": {
  "check": "run-s -c lint check:tsc test",
  "check:tsc": "tsc --project tsconfig.test.json",
  "start": "react-native start",
  "test": "jest --config=jest.json",
  "test:watch": "jest --watch",
  "coverage": "run-p coverage:start coverage:open",
  "coverage:start": "http-serve coverage/lcov-report -p 3000",
  "coverage:open": "opn http://localhost:3000/",
  "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts'",
  "avd": "scripts/avd.js",
  "android": "scripts/android.js",
  "android:build": "scripts/androidBuild.sh",
  "android:clean": "cd android && ./gradlew clean",
  "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
  "ios": "run-p start ios:run",
  "ios:run": "react-native run-ios",
  "ios:deploy": "./scripts/ios.sh",
  "generate:component": "yo local:component",
  "clean": "npm run clean:rn && npm run typescript:clean",
  "clean:rn": "rm -rf $TMPDIR/react-*",
  "clean:all": "git clean -f -x -e=node_modules/* -e=.vscode/*",
  "test:tsc": "tsc --project tsconfig.test.json",
  "typescript:clean": "rm App/**/*.js && rm App/**/*.js.map",
  "storybook": "run-p storybook:start storybook:connect storybook:browser storybook:android",
  "prestorybook:start": "rnstl --searchDir src --pattern \"**/*.story.tsx\"",
  "storybook:start": "storybook start -p 7007",
  "storybook:connect": "adb reverse tcp:7007 tcp:7007",
  "storybook:browser": "sleepms 5000 && opn 'http://localhost:7007/'",
  "storybook:android": "sleepms 5000 && npm run android"
 },
 "dependencies": {
  "react": "^16.0.0",
  "react-dom": "^16.0.0",
  "react-native": "^0.49.1",
  "react-navigation": "^1.0.0-beta.13",
  "redux": "^3.7.2"
 },
 "devDependencies": {
  "@storybook/addon-actions": "^3.2.12",
  "@storybook/addon-options": "^3.2.12",
  "@storybook/addon-storyshots": "^3.2.12",
  "@storybook/react-native": "^3.2.12",
  "@types/enzyme": "^2.8.12",
  "@types/jest": "^21.1.2",
  "@types/react": "^15.6.4",
  "@types/react-native": "^0.49.2",
  "@types/react-navigation": "^1.0.21",
  "@types/react-redux": "^5.0.10",
  "@types/redux": "^3.6.0",
  "@types/redux-thunk": "^2.1.0",
  "@types/storybook__addon-options": "^3.2.0",
  "babel-jest": "21.2.0",
  "babel-plugin-module-alias": "^1.6.0",
  "babel-preset-react-native": "4.0.0",
  "chalk": "^2.1.0",
  "core-js": "^2.5.1",
  "enzyme": "^3.1.0",
  "enzyme-adapter-react-16": "^1.0.1",
  "generator-local": "./yeoman",
  "http-serve": "^1.0.1",
  "jest": "21.2.1",
  "jest-cli": "^21.2.1",
  "npm-run-all": "^4.1.1",
  "opn-cli": "^3.1.0",
  "react-native-storybook-loader": "^1.6.0",
  "react-native-typescript-transformer": "^1.1.4",
  "react-redux": "^5.0.6",
  "react-test-renderer": "16.0.0-beta.5",
  "reactxp": "^0.46.3",
  "redux-thunk": "^2.2.0",
  "simple-terminal-menu": "^1.1.3",
  "sleep-ms": "^2.0.1",
  "through2": "^2.0.3",
  "ts-jest": "^21.1.2",
  "tslint": "^5.7.0",
  "tslint-config-standard": "^6.0.1",
  "typescript": "^2.5.3",
  "yargs": "^9.0.1",
  "yo": "^2.0.0"
 },
 "jest": {
  "preset": "react-native"
 }
}
